# de-21
#include <stdio.h>

unsigned long long factorial(unsigned int n) {
    if (n == 0) {
        return 1;
    } else {
        return n * factorial(n-1);
    }
}

int main() {
    unsigned int n = 10;
    unsigned long long result = factorial(n);
    printf("Tich cac so tu 1 den %u la: %llu\n", n, result);
    return 0;
}
